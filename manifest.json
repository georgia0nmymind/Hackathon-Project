{
  "name": "Random Simpsons Episode Generator",
  "options_page": "options.html",
  "version": "0.1.0",
  "description": "Ever in the mood for a random episode of The Simpsons? Look no further than RSEG!",
  "permissions": ["activeTab", "storage", "http://*/", "https://*/"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/donut.png"
    }
  },
  "icons": {
    "128": "images/donut.png"
  },
  "manifest_version": 2
}
